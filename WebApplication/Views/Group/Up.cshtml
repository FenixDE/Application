
@{
    ViewBag.Title = "Up";
}

@using (Html.BeginForm("Up", "Faculty", FormMethod.Post))
{
    <label for="Name">Название</label>
    <input type="text" class="form-control" id="Name" name="Name" aria-describedby="name" placeholder="Введите название"
           value="@ViewBag.faculty.Name" />
       <label for="Info">Описание</label>   
       <input type="text" class="form-control" id="Info" name="Info" aria-describedby="info" placeholder="Введите название"
           value="@ViewBag.faculty.Info" />
       <input type="hidden" value="@ViewBag.faculty.ID" />
       <input type="submit" class="btn btn-submit" value="Изменить" />
}

